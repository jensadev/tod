{
  "name": "tod",
  "version": "1.0.0",
  "description": "Tema omr√•de del",
  "scripts": {
    "color-tokens": "a11y-color-tokens --outputDirPath='src/scss' --colorTokensPath='src/_theme/color-tokens.js'",
    "watch:sass": "sass --no-source-map --watch src/scss:public/css",
    "watch:eleventy": "eleventy --serve",
    "watch:rollup": "rollup -c -w",
    "build:sass": "sass --no-source-map src/scss:public/css",
    "build:eleventy": "eleventy",
    "build:rollup": "rollup -c",
    "start": "npm-run-all color-tokens build:sass --parallel watch:*",
    "build": "npm-run-all color-tokens build:sass --parallel build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jensnti/tod.git"
  },
  "keywords": [
    "education"
  ],
  "author": "Jens Andreasson",
  "license": "GPL 3",
  "bugs": {
    "url": "https://github.com/jensnti/tod/issues"
  },
  "homepage": "https://github.com/jensnti/tod#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@11ty/eleventy-navigation": "^0.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "a11y-color-tokens": "^0.4.0",
    "bootstrap": "^5.0.0-beta3",
    "canvas-confetti": "^1.4.0",
    "elasticlunr": "^0.9.5",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-link-attributes": "^3.0.0",
    "nice-color-palettes": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.53.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.34.0"
  }
}
